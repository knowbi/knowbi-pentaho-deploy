        <jsScript_script>//Script here

var index = indexOf(path, 'content');
var deployPath = substr(path, index+7);
deployPath = deployPath.replace(/\\/g, '/');
deployPath = replace(deployPath, '/', ':');
deployPath = replace(deployPath, ' ', '%20');

var baseURL = getVariable('pentaho.base.url', '');
var deployURL = baseURL + '/api/repo/files/' + deployPath + ':' + filename</jsScript_script>
